{
    "browser": true,
    "globals": {
        "$": false,
        "GM_getValue": false,
        "GM_setValue": false,
        "GM_deleteValue": false
    },

    "eqeqeq": true,
    "freeze": true,
    "indent": 4,
    "latedef": true,
    "undef": true,
    "unused": true,
    "immed": true,
    "trailing": true,
    "maxcomplexity": 50,
    //"maxlen": 120,

    "-W065": false, // Missing radix parameter to parseInt (defaults to 10)
    "-W100": false // Silently deleted characters (in locales)
}
